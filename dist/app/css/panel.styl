.panel
  background-color: $white
  border-radius: 10px
  position: relative
  margin-bottom: 20px
  .panel-head
    background: $pink
    color: #fff
    border-bottom: 1px solid $light-gray
    border-top-left-radius: 10px
    border-top-right-radius: 10px
    h5
      padding: 15px
      text-transform: uppercase
      font-weight: 600
      font-size: 1.4em
      color: $white
  .panel-body
    border-radius: 10px
    overflow-y: auto
    overflow-x: hidden
    padding: 20px 15px

